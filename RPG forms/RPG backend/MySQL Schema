CREATE DATABASE RPG;
USER RPG;

CREATE TABLE IF NOT EXISTS `rpg`.`classbase` (
  `id_class` INT(11) NOT NULL AUTO_INCREMENT,
  `ClasseNome` CHAR(255) NULL DEFAULT NULL,
  `strengh` INT(11) NULL DEFAULT NULL,
  `intelligence` INT(11) NULL DEFAULT NULL,
  `charisma` INT(11) NULL DEFAULT NULL,
  `speed` INT(11) NULL DEFAULT NULL,
  `HP` INT(11) NULL DEFAULT NULL,
  PRIMARY KEY (`id_class`))
ENGINE = InnoDB
AUTO_INCREMENT = 0
DEFAULT CHARACTER SET = UTF-8
